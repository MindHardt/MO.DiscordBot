{
    "Discord": {
        "Token": "INSERT YOUR TOKEN",
        "OwnerIds": []
    },
    "Faker": {
        "Locale": "ru"
    },
    "Serilog": {
        "Using":  [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
        "MinimumLevel": "Information",
        "Override": {
            "Microsoft.AspNetCore": "Warning"
        },
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "File",
                "Args": {
                    "path": "Logs/log-.log",
                    "rollingInterval": "Day"
                }
            },
            {
                "Name": "Seq",
                "Args": {
                    "serverUrl": "http://localhost:5341"
                }
            }
        ],
        "Properties": {
            "Application": "MO.DiscordBot"
        }
    }
}